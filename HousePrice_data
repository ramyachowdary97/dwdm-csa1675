data<-read.csv("HousePrice_data.csv")
View(data)
mean(data$bedrooms)
median(data$price)
mode(data$condition)
summary(data$country)
IQR=(data$bathrooms)
IQR(data$bathrooms)
quantile(data$condition,0.25)
range(data$view)
mean(range(data$view))
Lf<-quantile(data$bathrooms,0.25)-1.5*(IQR(data$bathrooms))
print(Lf)
Uf<-quantile(data$bathrooms,0.25)+1.5*(IQR(data$bathrooms))
print(Uf)
outlier_values<-boxplot.stats(data$price)$out
print(outlier_values)
